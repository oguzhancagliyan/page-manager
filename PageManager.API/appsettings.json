{
  "Logging": {
    "LogLevel": {
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "NodeUris": "http://elasticsearch:9200",
          "AutoRegisterTemplate": true,
          "IndexFormat": "pagemanager-logs-{0:yyyy.MM.dd}",
          "TypeName": "_doc",
          "EmitEventFailure": "WriteToSelfLog",
          "NumberOfShards": 1,
          "NumberOfReplicas": 0
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "Https": {
    "Enabled": false
  },
  "AllowedHosts": "*"
}
